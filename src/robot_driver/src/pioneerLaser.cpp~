#include "ros/ros.h"
#include "geometry_msgs/Twist.h"

using namespace std;
using namespace ros;

int main(int argc, char **argv)
{
  init(argc,argv, "pioneer_laser_node");
  NodeHandle handle;
  Publisher pub_object = handle.advertise<geometry_msgs::Twist>("/RosAria/cmd_vel", 1);
  Rate loop_rate(10);
  geometry_msgs::Twist pub_msg;

  for(int i=0; i< 50; i++)
  {
    loop_rate.sleep();
    pub_msg.linear.x=0.05;
    pub_object.publish(pub_msg);

  }

  pub_msg.linear.x = 0;
  pub_object.publish(pub_msg);
  return 0;
}
